{
  "name": "lullabot/drupal-symfony-lock",
  "description": "Use Drupal's lock backends with Symfony's locking API.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
  {
    "name": "Andrew Berry",
    "email": "deviantintegral@gmail.com"
  }
  ],
  "require": {
    "php": ">=7.0",
    "drupal/core": "^8.5",
    "symfony/lock": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Lullabot\\DrupalSymfonyLock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lullabot\\DrupalSymfonyLock\\Tests\\": "tests/src/"
    }
  },
  "extra": {
    "composer-version": "^1.6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "drupal/coder": "^2.12",
    "squizlabs/php_codesniffer": "^2.8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ]
}
